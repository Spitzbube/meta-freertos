
all: $(FREERTOS_LIB)

$(FREERTOS_LIB): $(FREERTOS_OBJS) $(FREERTOS_PORT_OBJS) $(FREERTOS_MEMMANG_OBJS)
	$(AR) rcs ../$@ $^

$(FREERTOS_OBJS):%.o:$(FREERTOS_SRC)%.c
	$(CC) $< $(CFLAGS) -c -o $@

$(FREERTOS_PORT_OBJS):%.o:$(FREERTOS_PORT_SRC)%.c
	$(CC) $< $(CFLAGS) -c -o $@

$(FREERTOS_MEMMANG_OBJS):%.o:$(FREERTOS_MEMMANG_SRC)%.c
	$(CC) $< $(CFLAGS) -c -o $@
	
